{
  "name": "website-selling-cosmetics-v2",
  "private": true,
  "version": "1.0.0",
  "description": "Websit Selling Cosmetics v2",
  "main": "index.js",
  "repository": "https://github.com/QUANG221222/Website-Selling-Cosmetics-v2.git",
  "author": "WangCoder2k5",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "clean-dist": "rm -rf dist && mkdir dist",
    "build-babel": "babel ./src -d ./dist --extensions \".ts\"",
    "build": "npm run clean-dist && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node ./dist/server.js",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node --extensions \".ts\" ./src/server.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.27.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.36.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "mongodb": "^6.20.0"
  }
}
